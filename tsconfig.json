{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "types": ["astro/client"],
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/svg/*": ["src/svg/*"],
      "@/libs/*": ["src/libs/*"],
      "@/assets/*": ["src/assets/*"],
      "@/*": ["./*"],
    }
  }  
}
