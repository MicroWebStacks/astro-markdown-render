---
import {relAssetToUrl} from '../libs/utils'

export interface Props {
    node: object;
    path: string;
}

const { node, path} = Astro.props as Props;
const {title, url, alt} = node;

const url_abs = relAssetToUrl(url, path)
console.log(` * image at '${url_abs}'`)
---
<img src={url_abs} alt={alt} title={title} />
